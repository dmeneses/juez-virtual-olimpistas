<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="Bootstrap.php">
    <testsuites>
        <testsuite name="VirtualJudge">
            <directory>./ApplicationTest</directory>
        </testsuite>
    </testsuites>
    
 <logging>
  <log type="coverage-html" target="../../../build/logs/report" charset="UTF-8"
       highlight="false" lowUpperBound="35" highLowerBound="70"/>
  <log type="coverage-clover" target="../../../build/logs/coverage.xml"/>
  <log type="coverage-php" target="../../../build/logs/coverage.serialized"/>
  <log type="junit" target="../../../build/logs/logfile.xml" logIncompleteSkipped="false"/>
</logging>
</phpunit>